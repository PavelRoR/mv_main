*
	margin 0
	padding 0
	outline none

a
	color: #fefefe
	cursor: no-drop
	&:focus
		outline: 0
p
	line-height: 1
.video_wrapper
	margin 0 auto
	position relative
	&:before
		content ''
		height 0
		padding-top 56.25%
		display block
	img,
	iframe
		position absolute
		width 100%
		height 100%
		display block
		background-color #fff
		border-radius 3px
		border: 0
		top 0
		left 0
		right 0
		bottom 0
		cursor pointer



@import 'parts/header'
@import 'parts/main_screen'
@import 'parts/footer'