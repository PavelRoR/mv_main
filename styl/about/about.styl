.team_subtitle
    font-size: 20px;
    font-family: "HelveticaNeueCyr";
    color: rgba(85, 94, 124, 0.8);
    line-height: 1.45;
    margin: 31px 0 58px
    max-width 600px
.team_card
    border-radius: 3px;
    background-color: #fff
    box-shadow: 0px 0px 17px 0px rgba(0, 0, 0, 0.2);
    width: 554px;
    height: 370px;
    margin-bottom: 30px
    position relative
    padding: 58px 30px
    z-index 9
    cursor pointer
    overflow hidden
    h5
        font-size: 22px;
        font-family: "HelveticaNeueCyrBold", sans-serif;
        color: rgb(37, 37, 37);
        line-height: 1.1;
        margin-bottom: 16px
    p
        font-size: 16px;
        font-family: "HelveticaNeueCyrRoman", sans-serif;
        color: rgba(85, 94, 124, 0.8);
        line-height: 1.4
        max-width 225px
    .arrow_more
        position: absolute
        left: 30px
        bottom: 155px
    .coach
        position: absolute
        bottom 0
        right 0
        height calc(100% - 20px)
        width auto
        background-position top left
        background-repeat no-repeat
        z-index -1
