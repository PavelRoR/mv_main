.title_block
    height 400px
    position: relative
    padding 80px 40px 0
    margin 0 -15px
    h5
        font-size: 30px;
        font-family: "HelveticaNeueCyrBlack", sans-serif;
        color: rgb(37, 37, 37);
        line-height: 1.4;
        margin: 0 0 30px
        position relative
        z-index 9
        &+p
            font-size: 15px;
            font-family: "HelveticaNeueCyrRoman", sans-serif;
            color: rgba(68, 71, 103, 0.8);
            line-height: 1.55;
            max-width 210px
            position relative
            z-index 9
    img
        height: inherit;
        position: absolute
        bottom: 0
        right: 0
        z-index 0
.competences
    margin: 0 -15px
    background-color: #F7F8FA
    padding: 60px 40px 45px
    h6
        font-size: 27px;
        font-family: "HelveticaNeueCyrBold";
        color: rgb(37, 37, 37);
        line-height: 1
        margin-bottom: 33px
    ul
        list-style disc
        margin-left: 20px
        li
            color: #363DE4
            font-size: 18px;
            font-family: "HelveticaNeueCyrRoman", sans-serif;
            line-height: 1.6
            margin-bottom: 20px
            span
                color: rgba(68, 71, 103, 0.8);
    p
        color: rgba(68, 71, 103, 0.8);
        font-size: 18px;
        font-family: "HelveticaNeueCyrRoman", sans-serif;
        line-height: 1.6
        margin-bottom: 20px
.contact_section
    padding 35px 40px 50px
    p
        font-size: 18px;
        font-family: "HelveticaNeueCyrMedium", sans-serif;
        color: rgba(68, 71, 103, 0.8);
        line-height: 1.6;
        margin-bottom: 20px
        &:last-of-type
            margin-bottom: 55px
    .coach_else
        font-size: 18px;
        font-family: "HelveticaNeueCyrMedium", sans-serif;
        color: rgba(68, 71, 103, 0.8);
        line-height: 1.6;
        margin-bottom: 55px
.coach_else_list
    margin-bottom: 55px
    li
        font-size: 18px;
        font-family: "HelveticaNeueCyrMedium", sans-serif;
        color: rgba(68, 71, 103, 0.8);
        margin-bottom: 20px
        list-style-type disc
        line-height: 1.6
.coach_contacts
    overflow hidden
    h6
        font-size: 27px;
        font-family: "HelveticaNeueCyrBold",sans-serif;
        color: rgb(37, 37, 37);
        line-height: 1
        margin-bottom: 40px
    a
        font-size: 18px;
        font-family: "HelveticaNeueCyrRoman", sans-serif;
        color: rgb(29, 36, 43);
        line-height: 1.444
        margin-bottom: 0
        float left
        margin-right: 35px
        span
            color #5452fe
            font-family: "HelveticaNeueCyrMedium", sans-serif;
        &:last-child
            margin-right: 0
        &:hover
            text-decoration: underline